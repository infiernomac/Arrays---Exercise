using System;

namespace _2._Train
{
	class Program
	{
		static void Main(string[] args)
		{
			int trains = int.Parse(Console.ReadLine());

			int sum = 0;

			for (int i = 0; i < trains; i++)
			{
				int people = int.Parse(Console.ReadLine());

				sum += people;

				Console.Write($"{people} ");
			}
			Console.WriteLine("");
			Console.WriteLine(sum);
		}
	}
}
